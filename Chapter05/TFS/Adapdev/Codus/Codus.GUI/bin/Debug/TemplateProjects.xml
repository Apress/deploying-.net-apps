	<TemplateProject solution="DAO">
		<Name>DAO Framework - C#</Name>
		<Description>Generates the data access tier for a given database.</Description>
		<Author>Adapdev.Technologies</Author>
		<Version>v1.0</Version>
		<Copyright>2005</Copyright>
		<Editor assembly="templates\adapdev\Codus.Templates.Adapdev.dll" class="Codus.Templates.Adapdev.Editor" />
		<Templates>
			<Template type="nvelocity" templateFile="templates\adapdev\cs\nant.build" outputFileName="nant"
				outputFileExtension="build" outputDirectory="" overWrite="true" project="">
				<deps>
					<dep key="generatenant" type="eq" value="true" />
				</deps>
			</Template>
			<Template type="nvelocity" templateFile="templates\adapdev\cs\runbuild.bat" outputFileName="runbuild"
				outputFileExtension="bat" outputDirectory="" overWrite="true" project="">
				<deps>
					<dep key="generatenant" type="eq" value="true" />
				</deps>
			</Template>
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\storedprocedures.tsql"
				outputFileName="codusprocedures" outputFileExtension="sql" outputDirectory="" overWrite="true"
				project="">
				<deps>
					<dep key="sqltypesps" type="eq" value="true" />
				</deps>
			</Template>
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\entity.tcs" outputFileName="${table}Entity"
				outputFileExtension="cs" outputDirectory="src\Entities" className="${table}Entity" overWrite="false"
				project="DAO" />
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\entitybase.tcs" outputFileName="${table}EntityBase"
				outputFileExtension="cs" outputDirectory="src\Entities\Base" className="${table}EntityBase"
				overWrite="true" />
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\entitycollection.tcs" outputFileName="${table}EntityCollection"
				outputFileExtension="cs" outputDirectory="src\Collections" className="${table}EntityCollection"
				overWrite="true" project="DAO" />
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\entitydictionary.tcs" outputFileName="${table}EntityDictionary"
				outputFileExtension="cs" outputDirectory="src\Collections" className="${table}EntityDictionary"
				overWrite="true" project="DAO" />
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\entityenumerator.tcs" outputFileName="${table}EntityEnumerator"
				outputFileExtension="cs" outputDirectory="src\Collections" className="${table}EntityEnumerator"
				overWrite="true" project="DAO" />
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\abstractdao.tcs" outputFileName="${table}DAO"
				outputFileExtension="cs" outputDirectory="src\DAO" className="${table}DAO" overWrite="false"
				project="DAO" />
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\abstractdaobase.tcs" outputFileName="${table}DAOBase"
				outputFileExtension="cs" outputDirectory="src\DAO\Base" className="${table}DAOBase" overWrite="true" />
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\dbconstants.tcs" outputFileName="DbConstants"
				outputFileExtension="cs" outputDirectory="src\DAO" className="DbConstants" overWrite="true"
				project="DAO" />
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\connectionfactory.tcs"
				outputFileName="ConnectionFactory" outputFileExtension="cs" outputDirectory="src\DAO"
				className="ConnectionFactory" overWrite="true" project="DAO" />
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\zanebug.tcs" outputFileName="${table}DAOTest"
				outputFileExtension="cs" outputDirectory="src\Tests.Zanebug" className="${table}DAOTest"
				overWrite="true" project="DAOTestsZanebug">
				<deps>
					<dep key="generatezanebug" type="eq" value="true" />
				</deps>
			</Template>
			<!-- For Testing
            <Template type="nvelocitytable" templateFile="templates\adapdev\cs\development_nunit.tcs" outputFileName="${table}DAOTest" outputFileExtension="cs" outputDirectory="src\Tests.NUnit" className="${table}DAOTest" overWrite="true" project="DAOTestsNUnit">
                <deps>
                    <dep key="generatenunit" type="eq" value="true" />
                </deps>
            </Template>
            -->
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\nunit.tcs" outputFileName="${table}DAOTest"
				outputFileExtension="cs" outputDirectory="src\Tests.NUnit" className="${table}DAOTest"
				overWrite="true" project="DAOTestsNUnit">
				<deps>
					<dep key="generatenunit" type="eq" value="true" />
				</deps>
			</Template>
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\mockentity.tcs" outputFileName="Mock${table}"
				outputFileExtension="cs" outputDirectory="src\MockEntities" className="Mock${table}" overWrite="true"
				project="DAO" />
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\webservice.tcs" outputFileName="${table}WebService"
				outputFileExtension="cs" outputDirectory="src\WebServices" className="${table}WebService"
				overWrite="true" project="WebServices">
				<deps>
					<dep key="generatewebservice" type="eq" value="true" />
				</deps>
			</Template>
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\webservice.asmx.tcs" outputFileName="${table}WebService"
				outputFileExtension="asmx" outputDirectory="src\WebServices" className="${table}WebService"
				overWrite="true" project="WebServices">
				<deps>
					<dep key="generatewebservice" type="eq" value="true" />
				</deps>
			</Template>
			<Template type="nvelocitytable" templateFile="templates\adapdev\cs\webservice.constants.tcs"
				outputFileName="WebServiceConstants" outputFileExtension="cs" outputDirectory="src\WebServices"
				className="WebServiceConstants" overWrite="true" project="WebServices">
				<deps>
					<dep key="generatewebservice" type="eq" value="true" />
				</deps>
			</Template>
		</Templates>
		<CustomCode>
			<Code assembly="" class="" />
		</CustomCode>
	</TemplateProject>
